class SinglyLinkedList:
    #constructor
    def __init__(self):
        self.head = None

    #method for setting the head of the Linked List
    def setHead(self,head):
        self.head = head

    def reverse(self):
        prev = None
        current = self.head
        while current != None:
            nextNode = current.next
            current.next = prev
            prev = current
            current = nextNode
        self.head = prev
